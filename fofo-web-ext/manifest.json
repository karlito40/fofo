{
  "manifest_version": 2,
  "name": "Fofo",
  "version": "0.2",
  "description": "The secret community",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
